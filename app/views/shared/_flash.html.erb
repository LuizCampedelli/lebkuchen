<% flash.each do |key, message| %>
  <div id="ale-not" class="alert alert-<%= key == 'notice' ? 'success' : 'danger' %> alert-dismissible fade show m-1" role="alert">
    <%= message %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
<% end %>


