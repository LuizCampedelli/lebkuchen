
<h1 id="page_title">Entre em contato conosco</h1>



  <%= simple_form_for @contact do |f| %>
  <div class="col-md-6">
      <%= f.input :name %><br>
      <%= f.input :email %><br>
      <%= f.input :message %><br>
      <div class="hidden">
          <%= f.label :nicname %><br>
          <%= f.text_field :nickname, hint: 'Leave this field blank' %>
      </div>



      <%= f.submit %>
  <% end %>
</div>
